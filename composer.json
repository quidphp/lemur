    "keywords": ["quidphp","quid","php-framework","php-cms","lemur-cms"],
    "homepage": "https://quidphp.com",
    "license": "MIT",
    "authors": [{
        "name": "Pierre-Philippe Emond",
        "email": "emondpph@gmail.com",
        "homepage": "https://emond.io",
        "role": "Developer"
    }],
    "require": {
        "php": ">=8.1.0",
        "quidphp/navigation": "dev-master",
        "quidphp/front": "dev-master",
        "quidphp/include": "dev-master",
        "quidphp/core": "dev-master",
        "tinymce/tinymce": "^6.0"
