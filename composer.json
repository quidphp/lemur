{
    "name": "quidphp/lemur",
    "type": "library",
    "description": "LemurCMS, a content management system built on top of the QuidPHP framework",
    "keywords": ["quidphp","quid","php-framework","php-cms","lemur-cms"],
    "homepage": "https://quidphp.com",
    "license": "MIT",
    "authors": [{
        "name": "Pierre-Philippe Emond",
        "email": "emondpph@gmail.com",
        "homepage": "https://emond.io",
        "role": "Developer"
    }],
    "require": {
        "php": ">=8.1.0",
        "quidphp/base": "5.31.*",
        "quidphp/main": "5.31.*",
        "quidphp/orm": "5.31.*",
        "quidphp/routing": "5.31.*",
        "quidphp/core": "5.31.*",
        "quidphp/node": "5.31.*",
        "quidphp/front": "5.31.*",
        "quidphp/include": "5.31.*",
        "verot/class.upload.php": "^2.0",
        "phpmailer/phpmailer": "^6.6",
        "tedivm/jshrink": "^1.4",
        "scssphp/scssphp": "^1.10",
        "tinymce/tinymce": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Quid\\Lemur\\": "src/",
            "Quid\\Test\\Lemur\\": "test/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}